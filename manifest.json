{
  "name": "Kyocera Address Book Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://172.16.20.242/*",
    "https://172.16.20.242/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "lib/icon.png"
  }
}
